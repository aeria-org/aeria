{
  "name": "@aeriajs/builtins",
  "version": "0.0.255",
  "description": "## Installation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo skipping",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc",
    "build:esm": "esbuild './src/**/*.ts' --outdir=dist --out-extension:.js=.mjs && pnpm build:esm-transform",
    "build:esm-transform": "pnpm -w esm-transform $PWD/dist",
    "write-icons": "pnpm -w write-icons"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./collections": {
      "types": "./dist/collections/index.d.ts",
      "import": "./dist/collections/index.mjs",
      "require": "./dist/collections/index.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.mjs",
      "require": "./dist/icons.cjs"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "bcryptjs": "^3.0.2"
  },
  "devDependencies": {
    "@aeriajs/common": "link:../common",
    "@aeriajs/core": "link:../core",
    "@aeriajs/entrypoint": "link:../entrypoint",
    "@aeriajs/types": "link:../types",
    "@aeriajs/validation": "link:../validation",
    "mongodb": "^6.16.0"
  },
  "peerDependencies": {
    "@aeriajs/common": "^0.0.143",
    "@aeriajs/core": "^0.0.255",
    "@aeriajs/entrypoint": "^0.0.147",
    "@aeriajs/types": "^0.0.125",
    "@aeriajs/validation": "^0.0.159"
  }
}
