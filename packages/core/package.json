{
  "name": "@aeriajs/core",
  "version": "0.0.136",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "TS_NODE_COMPILER_OPTIONS=\"$(cat ../compilerOptions.json)\" mocha -r ts-node/register tests/*.spec.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc",
    "build:esm": "esbuild './src/**/*.ts' --outdir=dist --out-extension:.js=.mjs && pnpm build:esm-transform",
    "build:esm-transform": "pnpm -w esm-transform $PWD/dist",
    "prepublishOnly": "pnpm build",
    "postinstall": "node dist/__scripts__/postinstall.js || true"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js",
      "types": "./dist/presets/index.d.ts"
    },
    "./presets/*": "./dist/presets/*"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aeriajs/builtins": "link:../builtins",
    "@aeriajs/common": "link:../common",
    "@aeriajs/entrypoint": "link:../entrypoint",
    "@aeriajs/http": "link:../http",
    "@aeriajs/security": "link:../security",
    "@aeriajs/types": "link:../types",
    "@aeriajs/validation": "link:../validation",
    "@types/jsonwebtoken": "^9.0.6",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongodb-memory-server": "^9.2.0"
  },
  "peerDependencies": {
    "@aeriajs/builtins": "^0.0.136",
    "@aeriajs/common": "^0.0.85",
    "@aeriajs/entrypoint": "^0.0.87",
    "@aeriajs/http": "^0.0.96",
    "@aeriajs/security": "^0.0.136",
    "@aeriajs/types": "^0.0.73",
    "@aeriajs/validation": "^0.0.88"
  },
  "dependencies": {
    "mongodb": "^6.5.0",
    "jsonwebtoken": "^9.0.2"
  },
  "optionalDependencies": {
    "mongodb-memory-server": "^9.2.0"
  }
}
