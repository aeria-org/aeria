collection TestCollection {
  owned "on-write"
  properties {
    name str
  }
  functions {
    get @expose("unauthenticated")
    getAll @expose(true)
    insert @expose
    remove @expose
    upload @expose
  }
  presets {
    crud
  }
}

contract ContractTest {
  payload {
    properties {
      name str
      contacts Topic
    }
  }
  response
    | Error { properties { name str } }
    | Result { properties { name str, age num } }
    | str
  roles true
}